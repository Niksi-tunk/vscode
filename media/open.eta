<% layout("./layout.eta") %>

<div class="mx-8 prose">
    <h1 class="text-[48px] text-center">My projects</h1>
    <div class="projects">
    <% it.projects.forEach(p => { %>
        <button onClick=buttonHandler("<%~ p %>") type="button" id="project-button">
            <%= p %>
        </button>
    <% }) %>
    </div>
</div> 
