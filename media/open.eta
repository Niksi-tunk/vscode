<% layout("./layout.eta") %>

<div class="prose p-4 text-center min-w-full">
  <h1 class="text-[48px]">My projects</h1>
</div>
<div class="projects">
	<% it.projects.forEach(p => { %>
			<button onClick=buttonHandler("<%~ p %>") type="button" id="project-button">
					<%= p %>
			</button>
	<% }) %>
</div>